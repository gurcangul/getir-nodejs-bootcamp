{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "radix": "off",
    "eqeqeq": "off",
    "no-restricted-globals": "off",
    "no-unused-vars": "off"
  }
}
